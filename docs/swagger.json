    "host": "localhost:8080",
    "basePath": "{{.BasePath}}",
    "paths": {
        "/order_books": {
            "get": {
                "description": "get your order book list",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "order book list",
                "parameters": [
                    {
                        "type": "string",
                        "description": "eg BTC_USD",
                        "name": "symbol",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/models.OrderBook"
                            }
                        }
                    }
                }
            },
            "post": {
                "description": "create an order book",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "new an order book",
                "parameters": [
                    {
                        "description": "order book form",
                        "name": "req",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "$ref": "#/definitions/forms.OrderBookForm"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "object",
                            "$ref": "#/definitions/dtos.OrderBookDTO"
                        }
                    }
                }
            }
        },
        "/order_books/{id}": {
            "put": {
                "description": "update an exist order book",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "update an order book",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "eg 1",
                        "name": "id",
                        "in": "path",
                        "required": true
                    },
                    {
                        "description": "order book model",
                        "name": "req",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "$ref": "#/definitions/models.OrderBook"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "object",
                            "$ref": "#/definitions/models.OrderBook"
                        }
                    }
                }
            },
            "delete": {
                "description": "cancel an exist order book",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "cancel an order book",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "eg 1",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "object",
                            "$ref": "#/definitions/utils.APIRes"
                        }
                    }
                }
            }
        }
    },
